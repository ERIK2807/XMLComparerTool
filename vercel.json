{
    "version": 2,
    "builds": [
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "public/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "public/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/papa",
            "dest": "public/server.js"
        }
    ]
}
